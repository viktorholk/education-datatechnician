<Window x:Class="ComboBoxAssignment.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ComboBoxAssignment"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="200">
    <StackPanel>
        <ComboBox Margin="10,10,10,10" x:Name="WebpagesComboBox"></ComboBox>
        <Label HorizontalContentAlignment="Center" x:Name="WebpageUrlLabel"></Label>
        <TextBlock Margin="10,10,10,10" x:Name="WebpageInfoTextBlock" ></TextBlock>

    </StackPanel>
</Window>
