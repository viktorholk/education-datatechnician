<Window x:Class="SOAP.AddWebpageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SOAP"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="New Webpage" Height="250" Width="200">
    <StackPanel>
        <Label Content="Name"></Label>
        <TextBox Margin="5,5,5,5" x:Name="NameTextBox" ></TextBox>
        <Label Content="Url"></Label>
        <TextBox Margin="5,5,5,5" x:Name="UrlTextBox" ></TextBox>
        <Label Content="Description"></Label>
        <TextBox Margin="5,5,5,5" x:Name="DescriptionTextBox" Height="50"></TextBox>
        <Button x:Name="AddWebpageButton" Content="Add" Click="AddWebpageButton_Click"></Button>

    </StackPanel>
</Window>
